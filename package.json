{
  "name": "dash-atom-docs",
  "version": "1.0.55",
  "description": "Generate Dash docset for Atom.io API docs",
  "author": "Ben Booth <bkbooth@gmail.com>",
  "repository": "git@github.com:bkbooth/dash-atom-docs.git",
  "keywords": [
    "Dash",
    "atom",
    "atom.io",
    "API",
    "docs"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s scrape build:docs build:db",
    "build:db": "node ./modules/build-db",
    "prebuild:db": "npm run clean:db",
    "clean:db": "rimraf Atom.docset/Contents/Resources/docSet.dsidx",
    "build:docs": "node ./modules/build-docs",
    "prebuild:docs": "npm run clean:docs",
    "clean:docs": "rimraf Atom.docset/*",
    "scrape": "httrack https://flight-manual.atom.io/api/v$npm_package_atom_version/ -O ./scraped +https://flight-manual.atom.io/api/* +https://flight-manual.atom.io/assets/*",
    "prescrape": "node ./modules/check-httrack && npm run clean:scrape",
    "clean:scrape": "rimraf scraped"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^3.0.0",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=8"
  },
  "atom_version": "1.39.1"
}
